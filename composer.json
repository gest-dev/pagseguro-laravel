{
    "name": "oliveiracorp/laravel-pagseguro",
    "description": "PagSeguro Checkout Transparente e Pagamento Recorrente(Assinaturas) no Laravel 5.*, 6.*, 7.*, 8.* e 9.*",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "pagseguro", "transparente", "recorrente", "assinaturas", "pix"],
    "authors": [
        {
            "name": "Gezer Oliveiracorp",
            "email": "gezerramo@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Oliveiracorp\\PagSeguro\\": "src/Oliveiracorp/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1.16",
        "illuminate/support": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oliveiracorp\\PagSeguro\\PagSeguroServiceProvider"
            ],
            "aliases": {
                "PagSeguro": "Oliveiracorp\\PagSeguro\\PagSeguroFacade",
                "PagSeguroRecorrente": "Oliveiracorp\\PagSeguro\\PagSeguroRecorrenteFacade",
                "PagSeguroBoleto": "Oliveiracorp\\PagSeguro\\PagSeguroBoletoFacade",
                "PagSeguroPix": "Oliveiracorp\\PagSeguro\\PagSeguroPixFacade"
            }
        }
    }
}
